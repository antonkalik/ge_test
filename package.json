{
  "name": "ge_test",
  "version": "1.0.0",
  "description": "Test task for Gaming Evolution",
  "main": "src/client/index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/antonkalik/ge_test.git"
  },
  "scripts": {
    "start": "npm run prod:build && node dist/server_bundle.js",
    "prod:build": "concurrently \"npm run prod:server\" \"npm run prod:client\"",
    "prod:server": "webpack --config ./webpack.server.js --mode production",
    "prod:client": "webpack --config ./webpack.client.js --mode production",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon ./src/server/index.js",
    "dev:client": "webpack-dev-server --mode development --config ./webpack.client.js --open",
    "test": "jest --config ./jest.config.js",
    "lint": "eslint ./src -c .eslintrc.json"
  },
  "author": "anton kalik",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "ws": "latest"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.2",
    "css-loader": "^3.4.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.2",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^8.0.0",
    "socket.io-client": "^2.3.0",
    "style-loader": "^1.1.2",
